<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1">
    <listener>
        <listener-class>main.java.MyListener</listener-class>
    </listener>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>main.java.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/ooo</url-pattern>
    </servlet-mapping>
</web-app>
